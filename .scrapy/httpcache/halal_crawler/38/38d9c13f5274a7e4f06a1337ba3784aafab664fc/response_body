<script type="text/javascript">
function do_menu1(URL,ty){
	document.frm.ty.value=ty;
	document.frm.action = URL;
	document.frm.target='_self';
	document.frm.submit();
}
function do_menu(ty){
	document.frm.ty.value=ty;
	document.frm.action = 'directory_standalone.php';
	document.frm.target='_self';
	document.frm.submit();
}
function do_cari(ty){
	document.frm.ty.value=ty;
	document.frm.action = 'directory_standalone.php';
	document.frm.target='_self';
	document.frm.submit();
}
</script>
