<script type="text/javascript">
function open_modal(URL, title){
	//alert(URL);
	emailwindow=dhtmlmodal.open('EmailBox', 'iframe', URL, title, 'width=850,height=500,center=1,resize=0,scrolling=1')
} //End "opennewsletter" function
