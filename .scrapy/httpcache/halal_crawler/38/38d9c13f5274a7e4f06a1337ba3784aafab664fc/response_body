<script language="JavaScript">
function openModal(URL,title) {
	if (window.showModalDialog) { 
		window.showModalDialog(URL,title, "dialogWidth:850;dialogHeight:500");  
	} else { 
		window.open(URL,title, 'height=850,width=500,toolbar=no,directories=no,status=no, linemenubar=no,scrollbars=no,resizable=no ,modal=yes');
	}
} 
</script>
